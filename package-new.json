{
  "name": "interactive-video-websocket-server",
  "version": "2.0.0",
  "description": "WebSocket server for interactive video experience with multi-show support and iOS optimization",
  "main": "server-new.js",
  "scripts": {
    "start": "node server-new.js",
    "start:old": "node server.js",
    "dev": "nodemon server-new.js",
    "dev:old": "nodemon server.js",
    "hls": "python tools/mp4_inspect.py --assets assets --to-hls",
    "hls:force": "python tools/mp4_inspect.py --assets assets --to-hls --force",
    "hls:transcode": "python tools/mp4_inspect.py --assets assets --to-hls --transcode",
    "mp4:faststart": "python tools/mp4_inspect.py --assets assets --faststart",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "keywords": [
    "websocket",
    "touchdesigner",
    "video",
    "ios",
    "interactive",
    "multimedia"
  ],
  "author": "HMH Kampagne",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/websocket-server.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/websocket-server/issues"
  }
}
